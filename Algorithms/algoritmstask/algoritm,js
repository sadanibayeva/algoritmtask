ixtyari 5 heqiqi ededden necesi tam,necesi natural,necesi kesr
let tamsay=0;
let kesrsay=0;
let arr=[];
for(let i=0;i<5;i++){
    arr[i]=+prompt(`${i+1}-ci ededi daxil edin: `);
}
for(let i=0;i<5;i++){
    if(Math.floor(arr[i])==arr[i] && arr[i]>0){
        console.log(`${arr[i] }-tam, musbet`);
        tamsay++;
    }
        else if(Math.floor(arr[i])==arr[i] && arr[i]<0){
            console.log(`${arr[i] }-tam, menfi`);
            tamsay++;
        }
            if(arr[i]!=Math.floor(arr[i]) && arr[i]>0){
            console.log(`${arr[i] }-heqiqi,kesr, musbet`);
            kesrsay++;
             }
            else if(arr[i]!=Math.floor(arr[i]) && arr[i]<0){
            console.log(`${arr[i] }-heqiqi,kesr, menfi`);
            kesrsay++;
             }
             else if(arr[i]==0){
                console.log(`${arr[i]}-sifir`)
            }
}
console.log("tam say:" + tamsay);
console.log("kesr say:"+ kesrsay);


