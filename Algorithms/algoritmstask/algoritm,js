for(var i=0;i<arr.length;i++){
    arr[i]=+prompt(`$arr{i}-ci ededi daxil edin:`);
    if(arr[i]>max)
{
        max=arr[i];
    }
}
console.log(max);
array doldurulmuyubsa arr[0]cisi diyende exception olur. buna gore de max or min=0 yazrq. amma array dolubsa her iki yol duzdu
hemde sen max=arr[0] yazirsan axi 0'ci index hemin vaxt yoxdur undefineddir. soram arrayi doldurursan, buna gore de undefined olur netice
