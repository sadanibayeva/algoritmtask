var reqem=131;
var str=reqem.toString();
let newstr=''
var isPalindrome = function(x) {
    for(var i=0;i<x.length;i++){
        newstr+=x[i]
    }
    let yeni=''
    for(var i=x.length-1;i>=0;i--){
        yeni+=x[i];
    }
    if(newstr==yeni){
        console.log("polindromdu");
    }
    else{
        console.log("deil");
    }
}
isPalindrome(str);



