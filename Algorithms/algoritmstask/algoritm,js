for(let i=0;i<=5;i++)
{
if(i%2==0){
    sumc+=i;
    sayc++;
}
else if(i%2==1){
    sumt+=i;
    sayt++;
}
}
tam;
edediortat=sumt/sayt;
edediortac=sumc/sayc;
console.log(edediortac)
