daxil edilen ededi sozle yazan func(mes->input:1, output:Bir)
let num=+prompt("eded daxil et")
function words(){
    let teklik = ['sifir','bir', 'iki', 'uc', 'dord', 'besh', 'alti', 'yeddi', 'sekkiz', 'doqquz'];
    let onluq = ['on', 'iyirmi', 'otuz', 'qirx', 'elli', 'altmish', 'yetmish', 'seksen', 'doxsan'];
    let tdey="";
    let odey="";
    let ydey="";

for(let i=0;i<10;i++){
    if(num%10==i){
        tdey=teklik[i]
    }
    else if(num%10==0){
        tdey=''
    }
    if(Math.floor(num/10)==i){
        odey=onluq[i-1]
    }
    else if(Math.floor((num%100)/10)==i){
        odey=onluq[i-1]
    }
    if(Math.floor(num/100)==i){
        ydey=teklik[i]+"yuz"
    }else  if(Math.floor(num/100)==1){
        ydey="yuz"
    }
    else if(num%100==0){
        ydey=''
    }
}
console.log(ydey+" "+odey+" "+tdey);
console.log(ydey);
}
console.log(words());
    if (num < 0) throw new Error('Negative numbers are not supported.');
  
    if (num === 0) return 'zero';
  
    //the case of 1 - 20
    if (num < 20) {
      return ones[num];
    }
  
    if (numString.length === 2) {
      return tens[numString[0]] + ' ' + ones[numString[1]];
    }
  
    //100 and more
    if (numString.length == 3) {
      if (numString[1] === '0' && numString[2] === '0')
        return ones[numString[0]] + ' hundred';
      else
        return ones[numString[0]] + ' hundred and ' + convert(+(numString[1] + numString[2]));
    }
  
    if (numString.length === 4) {
      var end = +(numString[1] + numString[2] + numString[3]);
      if (end === 0) return ones[numString[0]] + ' thousand';
      if (end < 100) return ones[numString[0]] + ' thousand and ' + convert(end);
      return ones[numString[0]] + ' thousand ' + convert(end);
    }



