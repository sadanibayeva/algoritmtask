        let say = 0;
        for (let j = i; j < soz.length; j++) {
          if (soz[i] == soz[j]) {
            say++;
          }
        }
        if (!arr.includes(soz[i])) {
          arr.push(soz[i]);
        }
        if (arr.length <= i) {
          break;
        }
          console.log(`${arr[i]}-${say}`);
      }
