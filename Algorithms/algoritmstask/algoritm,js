//Add two number
var l1=[2,4,3];
var l2=[5,6,4];
 var yenil1=[];
 var yenil2=[];
 var cem=[];
var addTwoNumbers = function(l1, l2) {
    for(var i=l1.length-1;i>=0;i--){
        yenil1+=l1[i];
        var sayi1=parseInt(yenil1)
    }
    for(var i=l2.length-1;i>=0;i--){
        yenil2+=l2[i];
        var sayi2=parseInt(yenil2)
    }
    cem=sayi1+sayi2;
    var sonuc=cem.toString();
    var sonarr=[];
    for(var i=sonuc.length-1;i>=0;i--){
        sonarr+=sonuc[i];
    }
    console.log([...sonarr]);
}
addTwoNumbers(l1,l2)



// tekrarlari cix hansi herf var onu qaytar
var s="abcdabcdffgghhh";
var yeni=s.split('');
var lengthOfLongestSubstring = function(s) {
   for(var i=0;i<s.length;i++){
       for(var j=i+1;j<s.length;j++){
           if(s[i]==s[j]){
                  yeni.splice(i,1);
                  i--;
                }
            }
        }
   console.log(yeni.length)
};
lengthOfLongestSubstring(yeni);



// MEDIAN
var nums1 = [1,7,6,5];
var nums2 = [2];
var mergedArray;
var findMedianSortedArrays = function(nums1, nums2) {
    mergedArray = nums1.concat(nums2);
    for (var i = 0; i < mergedArray.length; i++) {
        for (var j = 0; j < mergedArray.length; j++) {
            if (mergedArray[i] < mergedArray[j]) {
                var temp = mergedArray[j];
                mergedArray[j] = mergedArray[i];
                mergedArray[i] = temp;
            }
        }
    }
    if (mergedArray.length % 2 === 0) {
        // Əgər uzunluq cütdürsə, median iki orta elementin ortasıdır.
        var middle1 = mergedArray.length / 2;
        var middle2 = middle1 - 1;
        var median = (mergedArray[middle1] + mergedArray[middle2]) / 2;
        console.log("Median:", median);
    } else {
        // Əgər uzunluq təkdirsə, median orta elementdir.
        var middle = Math.floor(mergedArray.length / 2);
        var median = mergedArray[middle];
        console.log("Median:", median);
    }
}
findMedianSortedArrays(nums1, nums2);





const input="{}";
function isValid(s) {
    const stack = [];
    for (let i = 0; i < s.length; i++) {
        const char = s[i];
        if (char === '(') {
            stack.push(')');
        } else if (char === '{') {
            stack.push('}');
        } else if (char === '[') {
            stack.push(']');
        } else if (char !== stack.pop()) {
            return false;
        }
    }
    return stack.length === 0;
}
console.log(isValid(input));



