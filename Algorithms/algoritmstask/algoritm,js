var reqem=[2,3,4];
var plusOne = function(digits) {
    let yeni="";
    for(var i=0;i<digits.length;i++){
        yeni+=digits[i];
        // console.log(yeni)
    }
    var eded = Number(yeni);
       sonuc=eded+1;
    //    console.log(sonuc);
    var str = String(sonuc);
    var arr = [...str];
console.log(arr);
}
plusOne(reqem);



//palindromlugu yoxla
var reqem=131;
var str=reqem.toString();
var isPalindrome = function(x) {
    for(var i=0;i<x.length;i++){
        for(var j=x.length-1;j>=0;j--){
            if(x[i]==x[j]){
                console.log("bu palindromdur")
            }
            else{
                console.log("bu palindrom deil");
            }
        }
    }
}
isPalindrome(str);
