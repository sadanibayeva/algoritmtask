artiq i=1 olms olr ona gore i-- yazrqki i=0dan baslasn ve yene yoxlasn silinenden soraki elementden baslayaraq. 
task2.arrayin icindeki tekrarlanan elementn silinmesi
let arr=[4,48,7,4,48,4,2,46];
for(let i=0;i<arr.length;i++){
    //for(let j=0;j<arr.length;j++){
    for(let j=i+1;j<arr.length;j++){
        //j=i+1 cunki oznden soraki elem muqayise etsn deye
        // if(i!=j){
            {
            if(arr[i]==arr[j]){
                arr.splice(i,1);
              i--;}
                //soraki elem silinenin yerne dushr deye muqayise etmek olmr yaddan cxr
                // delete arr[i]; bosluq saxlayr meslehet deil
        }
    }
}
console.log(...arr);


arrayda eded olmyan elementleri sil
let arr=["a","b",7,"c",8,true,false,5,3];
for(let i=0;i<arr.length;i++){
    if(typeof arr[i]!="number")
        {
        arr.splice(i,1);
            i--;
        }
}     
console.log(arr)


sozu tersine cevren funksiya 1ciyol
//once
let soz=prompt("sozu daxil et")
for(let i=0;i<soz.length;i++){
    console.log(soz)
}
//sonra
let word=prompt("wordu daxil et");
let ters="";
for(let i=word.length-1;i>=0;i--){
        ters+=word[i];   
}
console.log(ters)


sozu tersine cevren funksiya 2ciyol
let soz=prompt("sozu daxil et")
function reverse(a){
    let ters="";
    for(let i=a.length-1;i>=0;i--){
        ters+=a[i];
    }
    console.log(ters)
}
reverse(soz)


