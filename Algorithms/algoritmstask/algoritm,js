10 natural ededden sade ve murekkebi ayir ve saylarini tap
let sadesay = 0;
let murekkebsay = 0;
for(let i=0;i<10;i++){
    let say=0;
for(let j=1;j<=i;j++){
    if(i%j==0){
        say++;
    }
    }
if(say>2){
    console.log(i+" "+"murekkebdir");
    murekkebsay++;
}
else if(say==2){
    console.log(i+" "+"sadedir");
    sadesay++;
}
else{
    console.log(i+" "+"ne sadedir ne murekkeb")
}
}
console.log("cemi sade say: " + sadesay);
console.log("cemi murekkeb say: " + murekkebsay);


