2 arrayin eyniliyini yoxlayin
1ciyol-bu yol sehvdi cunki, 1ci indexle 0ci indexi yoxluyur gorur ferqlidi alinmir
let arr1=[2,4,5,6,7,5]
let arr2=[3,4,6,7,2,1]
for(let i=0;i<arr1.length;i++){
    for(let j=i+1;j<arr2.length;j++){
        if(arr1[i]=arr2[j]){
            console.log(`${arr1[i],arr2[i]}-eynidir`)
        }
        else{
            console.log(`${arr1[i],arr2[i]}-eyni deil`)
        }
    }
}
2ciyol-bu dogru
const first = [2, 4, 6, 7, 1];
const second = [4, 1, 7, 6, 2];
const areEqual = (first, second) => {
   if(first.length !== second.length){
      return false;
   };
   for(let i = 0; i < first.length; i++){
      if(!second.includes(first[i])){
         return false;
      };
   };
       return true;
};
console.log(areEqual(first, second));


array elementleri azalan sirayla duzun sort metodundan istifade etmeden
let arr=[2,9,8,5,1];
for(let i=0;i<arr.length;i++){
    for(let j=0;j<arr.length;j++){
         if(arr[i]>arr[j]){
         a=arr[j];
         arr[j]=arr[i];
         arr[i]=a;
        }
    }
}
console.log(...arr)


