1den 100e qeder sade ve murekkeb ededleri tapin 
for(let i=1;i<100;i++){
  let say=0;
  for(let j=1;j<=i;j++){
    if(i%j==0){
        say++
    }
    }
    if(say>2){
        console.log(i+" "+"murekkebdir")
    }
    else if(say==2){
        console.log(i+" "+"sadedir")
    }
    else{
        console.log(i+" "+"ne sadedir ne murekkeb")
    }
}
    

verilmish 3ededden max ve min tapin
let a=8; b=5; c=2; max=0;min=0;
if(a>b && a>c ){
       max=a;
       if(b>c)
       min=c;
       else
       min=b;
}
else if(b>a && b>c){
       max=b;
       if(c>a)
       min=a;
       else
       min=c;
}
else if(c>a && c>b){
       max=c;
       if(a>b)
       min=b;
       else
       min=a
}
alert(max)
alert(min)


